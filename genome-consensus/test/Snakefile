rule genome_consensus:
    input:
        alignment="consensusmap.bam",
        ref = "NC_045512.fa",
        vcf = "filtered.vcf"
    output:
        consensus="results/consensus.fa",
    log:
        "log/test.log"
    params:
        mask = 5,
        mapping_quality = 20,
    wrapper:
        "file:../wrapper.py"
